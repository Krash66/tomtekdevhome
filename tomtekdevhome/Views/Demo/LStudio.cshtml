@{
    ViewBag.Title = "LAMP Application - Web Studio";
    Layout = "~/Views/Shared/_LayoutDemo.cshtml";
}

<div class="container body-content">
    <div class="jumbotron text-center">
        <h2>@ViewBag.Title</h2>
        <h3>@ViewBag.Message</h3>

        <p class="lead">This is a fully functioning <a href="https://en.wikipedia.org/wiki/LAMP_(software_bundle)" target="_blank">LAMP</a> Application utilizing JavaScript, PHP, MySQL, JSON, XML, HTML5, CSS and the <a href="https://dhtmlx.com/" target="_blank">dHTMLx JavaScript framework by DHX.</a> This is a fully functioning App and allows uploads downloads and script generation. See the source code here.</p>
        <p class="lead">
            <a href="https://github.com/Krash66/SQDWebStudio" class="btn btn-primary btn-lg" target="_blank">SQD Web Studio Source Code &raquo;</a>
            <a href="http://tomtekdev.com/SQDATA/Studio/indexMain.html" class="btn btn-primary btn-lg" target="_blank">Open Web Studio in a new Tab &raquo;</a>
        </p>
    </div>
</div>
<div class="panel panel-default">
    <div class="panel-heading text-center">Web Studio</div>
    <div class="panel-body" style="min-height: 800px">
        <iframe id="iframeStudio" style="height:780px; width:100%; margin:0px; border:0px; padding:0px" src="http://tomtekdev.com/SQDATA/Studio/indexMain.html" /><br />
    </div>
</div>

