@{
    ViewBag.Title = "Owner's Resume";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <h2>@ViewBag.Title</h2>
    <h3>@ViewBag.Message</h3>

    <p class="lead">
        If you would like to download Tom's resume in MS Word format, click the download button.<br /><br />
        <a href="../Tom-Karasch-Resume.doc" class="btn btn-primary btn-lg" title="">Download Now &raquo;</a>
    </p>
</div>

<div class="panel panel-default">
    <div class="panel-heading">Resume</div>
    <div class="panel-body" style="min-height: 500px">
        <object type="text/html" width="100%" height="480px" data="~/resume.htm">Tom Karasch's Resume</object>
    </div>
</div>